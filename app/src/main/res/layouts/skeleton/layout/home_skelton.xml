<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorPrimary">
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/wellcome_txt"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:background="@drawable/gray_solid_background_skeleton"
        android:textSize="17sp"
        android:textColor="@color/colorwhite"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_margin="24dp"
        android:fontFamily="@font/dana_fa_ultra_bold"
        />
    <LinearLayout
        android:id="@+id/linear_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/wellcome_txt"
        android:background="@drawable/text_bg"
        android:orientation="vertical">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_income"
                android:layout_width="48dp"
                android:layout_height="48dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@drawable/gray_solid_background_skeleton"
                android:scaleType="center"
                android:layout_margin="16dp"
                tools:ignore="VectorDrawableCompat" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/title_income"
                android:layout_width="80dp"
                android:layout_height="24dp"
                android:background="@drawable/gray_solid_background_skeleton"
                android:fontFamily="@font/dana_fa_light"
                app:layout_constraintStart_toEndOf="@+id/img_income"
                app:layout_constraintTop_toTopOf="parent"
                android:textColor="@color/colorTxtHint"
                android:layout_marginTop="14dp"
                android:textSize="12sp"
                android:layout_marginStart="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/price_txt"
                android:layout_width="120dp"
                android:layout_height="24dp"
                android:fontFamily="@font/dana_fa_bold"
                android:background="@drawable/gray_solid_background_skeleton"
                app:layout_constraintStart_toEndOf="@+id/img_income"
                app:layout_constraintTop_toBottomOf="@id/title_income"
                app:layout_constraintBottom_toBottomOf="@id/line"
                android:textColor="@color/colorwhite"
                android:textSize="17sp"
                android:layout_marginStart="16dp"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_arrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                app:srcCompat="@drawable/ic_left_arrow"
                android:scaleType="fitCenter"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:ignore="VectorDrawableCompat" />
            <View
                android:id="@+id/line"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/colorTxtGray"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@id/price_txt"
                app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_insurance_submit"
                android:layout_width="48dp"
                android:layout_height="48dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:background="@drawable/gray_solid_background_skeleton"
                android:scaleType="center"
                android:layout_margin="16dp"
                tools:ignore="VectorDrawableCompat" />
            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/title_insurance_submit"
                android:layout_width="80dp"
                android:layout_height="24dp"
                android:background="@drawable/gray_solid_background_skeleton"
                android:fontFamily="@font/dana_fa_light"
                app:layout_constraintStart_toEndOf="@+id/img_insurance_submit"
                app:layout_constraintTop_toTopOf="parent"
                android:textColor="@color/colorTxtHint"
                android:textSize="12sp"
                android:layout_marginTop="14dp"
                android:layout_marginStart="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="120dp"
                android:layout_height="24dp"
                android:background="@drawable/gray_solid_background_skeleton"
                app:layout_constraintStart_toEndOf="@+id/img_insurance_submit"
                app:layout_constraintTop_toBottomOf="@id/title_insurance_submit"
                android:textColor="@color/colorwhite"
                android:layout_marginTop="8dp"
                app:fontFamily="@font/dana_fa_bold"
                android:textSize="17sp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="16dp"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/img_arrow_insurance_submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                app:srcCompat="@drawable/ic_left_arrow"
                android:scaleType="fitCenter"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:ignore="VectorDrawableCompat" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/insurance_txt"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/linear_info"
        android:background="@drawable/gray_solid_background_skeleton"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="@color/colorwhite"
        app:fontFamily="@font/dana_fa_bold"
        android:textSize="15sp"
        android:layout_marginTop="64dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="16dp"/>
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/insurance_txt"
        android:layout_marginTop="16dp"
        android:columnCount="3"
        android:rowCount="2"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        >
        <LinearLayout
            android:id="@+id/third_insurance_btn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:orientation="vertical"
            android:background="@drawable/text_bg"
            android:gravity="center">
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/gray_solid_background_skeleton"
                android:scaleType="fitCenter"
                tools:ignore="VectorDrawableCompat"
                android:layout_marginTop="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="48dp"
                android:layout_height="8dp"
                android:textColor="@color/colorTxtGray"
                android:background="@drawable/gray_solid_background_skeleton"
                android:textSize="12sp"
                android:layout_marginTop="8dp"
                app:fontFamily="@font/dana_fa_regular"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/motorcycle_Insurance"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:orientation="vertical"
            android:background="@drawable/text_bg"
            android:gravity="center"
            android:layout_marginStart="16dp">
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/gray_solid_background_skeleton"
                android:scaleType="fitCenter"
                tools:ignore="VectorDrawableCompat"
                android:layout_marginTop="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="48dp"
                android:layout_height="8dp"
                android:textColor="@color/colorTxtGray"
                android:background="@drawable/gray_solid_background_skeleton"
                android:textSize="12sp"
                android:layout_marginTop="8dp"
                app:fontFamily="@font/dana_fa_regular"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:orientation="vertical"
            android:background="@drawable/text_bg"
            android:gravity="center"
            android:layout_marginStart="16dp">
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/gray_solid_background_skeleton"
                android:scaleType="fitCenter"
                tools:ignore="VectorDrawableCompat"
                android:layout_marginTop="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="48dp"
                android:layout_height="8dp"
                android:textColor="@color/colorTxtGray"
                android:background="@drawable/gray_solid_background_skeleton"
                android:textSize="12sp"
                android:layout_marginTop="8dp"
                app:fontFamily="@font/dana_fa_regular"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:orientation="vertical"
            android:background="@drawable/text_bg"
            android:gravity="center"
            android:layout_marginTop="16dp"
            >
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/gray_solid_background_skeleton"
                android:scaleType="fitCenter"
                tools:ignore="VectorDrawableCompat"
                android:layout_marginTop="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="48dp"
                android:layout_height="8dp"
                android:textColor="@color/colorTxtGray"
                android:background="@drawable/gray_solid_background_skeleton"
                android:textSize="12sp"
                android:layout_marginTop="8dp"
                app:fontFamily="@font/dana_fa_regular"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:orientation="vertical"
            android:background="@drawable/text_bg"
            android:gravity="center"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp">
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/gray_solid_background_skeleton"
                android:scaleType="fitCenter"
                tools:ignore="VectorDrawableCompat"
                android:layout_marginTop="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="48dp"
                android:layout_height="8dp"
                android:textColor="@color/colorTxtGray"
                android:background="@drawable/gray_solid_background_skeleton"
                android:textSize="12sp"
                android:layout_marginTop="8dp"
                app:fontFamily="@font/dana_fa_regular"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_columnWeight="1"
            android:orientation="vertical"
            android:background="@drawable/text_bg"
            android:gravity="center"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp">
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="40dp"
                android:layout_height="40dp"
                app:srcCompat="@drawable/gray_solid_background_skeleton"
                android:scaleType="fitCenter"
                tools:ignore="VectorDrawableCompat"
                android:layout_marginTop="16dp"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="48dp"
                android:layout_height="8dp"
                android:textColor="@color/colorTxtGray"
                android:background="@drawable/gray_solid_background_skeleton"
                android:textSize="12sp"
                android:layout_marginTop="8dp"
                app:fontFamily="@font/dana_fa_regular"
                android:layout_marginBottom="16dp"/>
        </LinearLayout>

    </GridLayout>


</androidx.constraintlayout.widget.ConstraintLayout>